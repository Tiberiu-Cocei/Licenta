<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.201" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane fx:id="loginPane" maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="loginButton" layoutX="377.0" layoutY="389.0" mnemonicParsing="false" text="Login" />
            <TextField fx:id="loginUsername" layoutX="326.0" layoutY="229.0" promptText="Username" />
            <PasswordField fx:id="loginPassword" layoutX="326.0" layoutY="275.0" promptText="Password" />
            <Text fx:id="loginText" layoutX="275.0" layoutY="178.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please login in order to access the admin panel." />
            <Text fx:id="messageText" layoutX="319.0" layoutY="354.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" />
      </children>
    </AnchorPane>
  </children>
</VBox>
